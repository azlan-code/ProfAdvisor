{
	"name": "Prof Advisor",
	"version": "1.1",
	"manifest_version": 2,
	"description": "Displays UConn professor ratings in Student Admin",
	"icons": {
		"128": "icon128.png",
		"48": "icon48.png",
		"16": "icon16.png"
	},
	"background": {
		"scripts": ["jquery-3.5.1.min.js", "background.js"],
		"persistent": false
	},
	"page_action": {
		"default_icon": "icon16.png",
		"default_title": "ProfAdvisor",
		"default_popup": "popup.html"
	},
	"permissions": [
		"declarativeContent",
		"https://student.studentadmin.uconn.edu/*",
		"https://solr-aws-elb-production.ratemyprofessors.com/*"
	]
}